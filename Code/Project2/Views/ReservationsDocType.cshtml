    <script src="~/scripts/scriptaculous.js" type="text/javascript"></script>
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
    <style type="text/css">
        #nav_links_background {
            background-color: #c0c0c0;
            width: 20em;
        }

        .auto-style1 {
            height: 23px;
        }
    </style>
    <style type="text/css">
        .roundedPanel {
            width: 500px;
            background-color: #E8E8E8;
            padding-top: 10px;
        }
    </style>
    @RenderSection("PageScript", false)
</head>
<body class="center">
    <div class="container" style="max-width:800px;">
        <br />
        <div class="row">
            <div class="col-xs-12 center" style="padding: 0 0 0 0;">
                <div style="float:right;">
                    <img src="~/Content/Images/phone.gif" />
                </div>
                <div style="float:left;">
                    <a href="/home">
                        <img src="~/Content/Images/ResortsLogo.gif" />
                    </a>
                </div>
            </div>
            <br />
            <br />
            <br />
        </div>
        <div class="row" id="SiteMenu">
            <div class="col-xs-12">
                @{Html.RenderAction("Index", "SiteTopNav");}
            </div>
        </div>
        <div class="row">
            @*<div class="col-lg-3 col-md-3 col-sm-3 col-xs-3" id="OwnersPalette">
                    @{Html.RenderAction("OwnersPalette", "OwnersPalette");}
                    @RenderSection("OwnerPaletteInfo", false)
                </div>*@
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                @RenderBody()
            </div>
        </div>
        <div class="row">
            <div>
                @{Html.RenderPartial("SiteFooter");}
            </div>
        </div>
    </div>
